<!DOCTYPE html>
{#% spaceless %#}
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Control de Indicadores Logísticos">
        <meta name="author" content="Tecno Punta">
        <title>{% block title %}Iniciar sección{% endblock %} </title>
        {% block stylesheets %}
                
                <link href="{{asset('bundles/app/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" >
                <link href="{{asset('bundles/app/css/bootstrap.css') }}" type="text/css" rel="stylesheet" >
                
                

                    <!-- MetisMenu CSS -->
                    
                    <link href="{{asset('bundles/app/css/metisMenu.min.css') }}" type="text/css" rel="stylesheet" >
                    <!-- Custom CSS -->
                    <link href="{{asset('bundles/app/css/sb-admin-2.css') }}" type="text/css" rel="stylesheet" >
                    <!-- Custom Fonts -->
                    <link href="{{asset('bundles/app/css/font-awesome.min.css') }}" type="text/css" rel="stylesheet" >
                    
                
                
            <style>
               a.linkBormey, a:hover.linkBormey, a:active.linkBormey, a:enabled.linkBormey {list-style: none; color: #8a6d3b;}
               .colorBormey {color: #8a6d3b;}
            </style>
        
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        
        
        
        
        {% block javascripts %}
        
        <script src="{{ asset('bundles/app/js/jquery.js') }}" ></script>
        <script src="{{ asset('bundles/app/js/metisMenu.min.js') }}" ></script>        
        <script src="{{ asset('bundles/app/js/sb-admin-2.js') }}" ></script>        
        <script src="{{ asset('bundles/app/js/bootstrap.min.js') }}" ></script>


        
        
        {% endblock %}
    </body>
    
    
</html>
{#% endspaceless %#}